{
	"Port" : 8080,
	"Handlers" : [{
	    "Type" : "Command",
		"Name" : "OS Version",
		"Cmd" : "uname -a",
		"URL" : "/os/uname",
		"Regex" : "^(\\w+)\\s+\\S+\\s+(\\S+)",
		"Submatches" : ["os", "version"]
	},
	{
		"Name" : "OS Uptime",
		"Cmd" : "uptime",
		"URL" : "/os/uptime",
		"foo" : "foo"
	},
	{
		"Name" : "OS vmstat",
		"Cmd" : "vmstat",
		"URL" : "/os/vmstat",
		"Regex" : "\\s+(\\d+)\\s+(\\d+)\\s+(\\d+)\\s+(\\d+)\\s+(\\d+)\n",
		"Submatches" : ["user", "system", "idle", "wait", "stolen"],
		"PollInterval" : "10s",
		"TimeSeries" : [{"RollUp" : 6, "Capacity" : 100},
						{"RollUp" : 10, "Capacity" : 100},
						{"RollUp" : 6, "Capacity" : 100}]
	},
	{
		"Name" : "OS iostat",
		"Cmd" : "iostat",
		"URL" : "/os/iostat"
	},
	{
		"Name" : "OS Procs",
		"Cmd" : "ps aux",
		"URL" : "/os/ps"
	}]
}